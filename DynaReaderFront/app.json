{
  "expo": {
    "name": "DynaReaderFront",
    "slug": "DynaReaderFront",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "dynareaderfront",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.danillitos.DynaReaderFront"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c18f51ca-a5f8-4afb-adfb-0ccc32ac8db7"
      }
    }
  }
}
